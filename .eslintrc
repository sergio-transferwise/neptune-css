{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-mixed-operators": 0,
    "no-use-before-define": 0,
    "jsx-a11y/label-has-for": [2, {
      "required": {
        "some": ["nesting", "id"]
      }
    }],
    "jsx-a11y/label-has-associated-control": [2, {
      "required": {
        "some": ["nesting", "id"]
      }
    }],
    "import/prefer-default-export": 0
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
